	</prefix>
	<!-- Überprüfen der Pflichtfelder -->
	<dependencies>
		<item refId="operation_id" value="">
			<required>
				<item itemId="protocol_id"/>
				<item itemId="host_id"/>
				<item itemId="user_id"/>
			</required>
			<optional>
				<item itemId=""/>
			</optional>
		</item>
		<item refId="operation_id" value="make_temp_directory">
			<required>
				<item/>
			</required>
		</item>
		<item refId="operation_id" value="remove_temp_directory">
			<required>
				<item/>
			</required>
		</item>
		<item refId="operation_id" value="execute">
			<required>
				<item itemId="jump_protocol_id"/>
				<item itemId="jump_host_id"/>
				<item itemId="jump_port_id"/>
				<item itemId="jump_user_id"/>
			</required>
			<optional/>
		</item>
		<item refId="ssh_auth_method_id" value="publickey">
			<required>
				<item itemId="ssh_auth_file_id"/>
			</required>
		</item>
		<item refId="jump_ssh_auth_method_id" value="publickey">
			<required>
				<item itemId="jump_ssh_auth_file_id"/>
			</required>
		</item>
	</dependencies>
	<!-- überprüfen der Abhängigkeiten mit Hilfe der boolean Operationen wie and und or-->
	<checkParameter>
		<check checkId="settings_and_profile_id" bool_operation="and">
			<item itemId="settings_id"/>
			<item itemId="profile_id"/>
		</check>
		<check checkId="ssh_proxy_id" bool_operation="and">
			<item itemId="ssh_proxy_host_id"/>
			<item itemId="ssh_proxy_port_id"/>
			<item itemId="ssh_proxy_user_id"/>
		</check>
		<check checkId="jump_ssh_proxy_id" bool_operation="and">
			<item itemId="jump_proxy_host_id"/>
			<item itemId="jump_proxy_port_id"/>
			<item itemId="jump_proxy_user_id"/>
		</check>
		<check checkId="file_spec_or_path_id" bool_operation="or">
			<item itemId="file_path_id"/>
			<item itemId="file_spec_id"/>
		</check>
	</checkParameter>
	<!-- überprüft ob die Parameterwerte gültig sind -->
	<checkParameterValue>
		<check itemId="protocol_id">
			<paramvalue value="ftp"/>
			<paramvalue value="sftp"/>
			<paramvalue value="ftps"/>
		</check>
		<check itemId="operation_id">
			<paramvalue value="send"/>
			<paramvalue value="receive"/>
			<paramvalue value="remove"/>
			<paramvalue value="execute"/>
			<paramvalue value="make_temp_directory"/>
			<paramvalue value="remove_temp_directory"/>
			<paramvalue value="delete"/>
			<paramvalue value="rename_local_files"/>
			<paramvalue value="install"/>
			<paramvalue value="delete_local_files"/>
			<paramvalue value="install_doc"/>
		</check>
		<check itemId="ssh_auth_method_id">
			<paramvalue value="publickey"/>
			<paramvalue value="password"/>
		</check>
	</checkParameterValue>
