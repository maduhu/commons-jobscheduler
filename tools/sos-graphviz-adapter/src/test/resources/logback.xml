<configuration>

   <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
       <encoder>
           <pattern>%d{dd HH:mm:ss,SSS} %-5p [%t] %c:%L: %m%n</pattern>
       </encoder>
   </appender>
   
   <root level="warn">
       <appender-ref ref="console" />
   </root>
   
   <logger name="org.apache.commons" level="warn" additivity="false">
       <appender-ref ref="console" />
   </logger>
   
</configuration>
