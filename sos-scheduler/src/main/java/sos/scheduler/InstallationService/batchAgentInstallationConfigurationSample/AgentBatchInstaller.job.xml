<?xml version="1.0" encoding="ISO-8859-1"?>

<job title="Installs or Updates Agents on several Server">
    <settings>
        <log_level><![CDATA[info]]></log_level>
    </settings>
    <params>
        <param name="local_dir"
               value="config\live\sos\batchAgentInstallation"/>


        <param name="installation_definition_file"
               value="config\live\sos\batchAgentInstallation\scheduler_agent_installations.xml"/>


        <param name="installation_job_chain"
               value="batchAgentInstallation/automatic_installation"/>
    </params>
    <script java_class="sos.scheduler.InstallationService.JSBatchInstallerJSAdapterClass"
            language="java"
            java_class_path=""/>
    <run_time/>
</job>
