<?xml version="1.0" encoding="iso-8859-1"?>
<!-- $Id$ -->

<param xmlns="http://www.sos-berlin.com/schema/scheduler_job_documentation_v1.1"
       xmlns:xhtml="http://www.w3.org/1999/xhtml"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       name="remote_dir"
       DefaultValue="."
       required="true"
	   deprecated="target_dir"
       DataType="SOSOptionFolderName">
	   
	       <indexterm index="remote_dir, parameter" display="false"></indexterm>
    <indexterm index="parameter, remote_dir" display="false"></indexterm>
	   
        <note language="de">
		    <title>Name des Ziel-Verzeichnisses</title>
		    <div xmlns="http://www.w3.org/1999/xhtml">
			<p>
            Verzeichnis am Target (FTP/SFTP Server), von dem bzw. zu dem Dateien übertragen werden.
			</p>
			<p>
            Per Voreinstellung wird das Home-Verzeichnis des Benutzers am FTP/SFTP Server verwendet.
			</p>
          </div>
        </note>
        <note language="en">
		    <title>Name of the Target Folder</title>
          <div xmlns="http://www.w3.org/1999/xhtml">
			<p>
            Directory at the FTP/SFTP server from which or to which files should be transferred.
			</p>
			<p>
            By default the home directory of the user at the FTP/SFTP server is used.
			</p>
          </div>
        </note>
      </param>