<param xmlns="http://www.sos-berlin.com/schema/scheduler_job_documentation_v1.1"
       xmlns:xhtml="http://www.w3.org/1999/xhtml"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       name="ssh_proxy_host"
       required="false"
       extends="proxy_host"
       see="param_host">
	   
	<indexterm index="SSH, ssh_proxy_host" display="false"></indexterm>
	   
	 <indexterm index="ssh_proxy_host, parameter" display="false"></indexterm>
    <indexterm index="parameter, ssh_proxy_host" display="false"></indexterm>

        <note language="de">
				  <title/>
          <div xmlns="http://www.w3.org/1999/xhtml">
		  <p>
            Der Wert des Parameters ist der Hostname oder die IP-Adresse eines Proxies, 
            über den die Verbindung zum SSH Server hergestellt wird.
            Die Verwendung von Proxies ist optional.
		  </p>
          </div>
        </note>
        <note language="en">
		    <title/>
          <div xmlns="http://www.w3.org/1999/xhtml">
		  <p>
            The value of this parameter is the host name or the IP address of a 
            proxy that is used in order to establish a connection to the SSH server. 
            The use of a proxy is optional.
		  </p>
          </div>
        </note>
      </param>