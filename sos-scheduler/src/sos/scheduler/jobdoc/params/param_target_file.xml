<param
	xmlns="http://www.sos-berlin.com/schema/scheduler_job_documentation_v1.1"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xhtml="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="target_file"
	Category="FileHandling" DataType="SOSOptionFileOrPathName" OptionName="target_file"
	Alias="target" default_value="." required="true">
	
    <indexterm index="target_file, parameter" display="false"></indexterm>
    <indexterm index="parameter, target_file" display="false"></indexterm>
	
	<note language="de"> 
		<title>Ziel-Datei oder -Verzeichnis</title>
		<p>
			Zieldatei oder Zielverzeichnis
            </p>
		<p>
			Wird
			<paramref>target_file</paramref>
			nicht angegeben, dann wird der Verzeichnispfad von
			<paramref>source_file</paramref>
			als Ziel verwendet.
		</p>
	</note>
	<note language="en">
        <title>Target-file or -folder</title>
		<p>
			Target file or directory
                        </p>
		<p>
			If the
			<paramref>target_file</paramref>
			is not specified, then the directory path of
			<paramref>source_file</paramref>
			is used as target.
		</p>
	</note>
    <xi:include href="../notes-de/ParameterSubstitution.xml"
        xmlns:xi="http://www.w3.org/2001/XInclude" parse="xml" />
	<xi:include href="../notes/ParameterSubstitution.xml"
		xmlns:xi="http://www.w3.org/2001/XInclude" parse="xml" />
	<xi:include href="../notes-de/ParameterMaskSubstitution.xml"
		xmlns:xi="http://www.w3.org/2001/XInclude" parse="xml" />
	<xi:include href="../notes/ParameterMaskSubstitution.xml"
		xmlns:xi="http://www.w3.org/2001/XInclude" parse="xml" />
</param>

 